        					<occurrences minimum="0" maximum="1"/>
        					<default></default>
        				</input>
        				<input name="verticalAlign" type="TextLine">
        					<label>Vertical align</label>
        					<occurrences minimum="0" maximum="1"/>
        					<default></default>
        				</input>
        				<item-set name="mediaQueries">
        					<label>Media query</label>
        					<occurrences minimum="0" maximum="0"/>
