        <field-set name="regionsFieldSet">
            <label>Regions</label>
            <items>
                <item-set name="regions">
        			<label>Region</label>
        			<occurrences minimum="1" maximum="12"/><!-- maximum should be same as number of regions -->
        			<items>
                        <!--input name="columns" type="CustomSelector">
                            <label>Columns</label>
