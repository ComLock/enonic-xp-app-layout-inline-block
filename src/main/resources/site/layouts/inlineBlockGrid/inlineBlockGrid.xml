                                    <label>Offset columns</label>
                                    <occurrences minimum="0" maximum="1"/>
                                    <default></default>
                                </input>
                                <input name="push" type="Long">
                                    <label>Push columns</label>
                                    <occurrences minimum="0" maximum="1"/>
                                    <default></default>
                                </input>
                                <input name="pull" type="Long">
                                    <label>Pull columns</label>
                                    <occurrences minimum="0" maximum="1"/>
                                    <default></default>
                                </input>
